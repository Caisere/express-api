# Database Configuration
# Replace with your actual PostgreSQL connection string
# Format: postgresql://username:password@host:port/database_name
DATABASE_URL="postgresql://username:password@localhost:5432/movies_db"

# JWT Configuration
# IMPORTANT: Generate a strong, random secret key for production
# You can use: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"

# JWT token expiration time
# Examples: "1h", "7d", "30d"
JWT_EXPIRES_IN="30d"

# Application Environment
# Options: development, production
NODE_ENV="development"

# Application local server port
PORT=8080

# Seed data creator ID
creatorId='56472hjkdnbfjksjj7893nfkjsu85'
